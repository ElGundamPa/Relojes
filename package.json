{
  "name": "reloj-ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-products": "tsx scripts/generate-products.ts",
    "generate-brands": "tsx scripts/generate-brands-collections.ts",
    "generate-all": "npm run generate-products && npm run generate-brands",
    "optimize-images": "tsx scripts/optimize-images.ts",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:win": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "kill-node": "taskkill /F /IM node.exe /T",
    "clean-all": "npm run kill-node && npm run clean:win"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/nprogress": "^0.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.19",
    "lucide-react": "^0.378.0",
    "next": "14.2.5",
    "next-auth": "^4.24.13",
    "next-themes": "^0.3.0",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.10.3",
    "stripe": "^14.25.0",
    "tailwind-merge": "^2.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.39",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3"
  }
}
